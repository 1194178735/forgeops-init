{
  "handler": {
    "type": "Router",
    "name": "_router",
    "baseURI": "http://web-lee:80",
    "capture": "all"
  },
  "heap": [
    {
      "name": "JwtSession",
      "type": "JwtSession"
    },
    {
      "name": "MyCapture",
      "type": "CaptureDecorator",
      "config": {
        "type": "Slf4jLogSink",
        "captureEntity": true,
        "captureExchange": true,
        "logSink": {
          "config": {
            "base": "org.forgerock.openig"
          }
        }
      }
    },
    {
      "name": "CommonClientHandler",
      "type": "ClientHandler",
      "config": {
        "_keyManager": null,
        "_trustManager": null,
        "connectionTimeout": "10 seconds",
        "connections": 64,
        "disableRetries": true,
        "disableReuseConnection": true,
        "hostnameVerifier": "ALLOW_ALL",
        "numberOfWorkers": 4,
        "soTimeout": "10 seconds"
      }
    }
  ],
  "monitor": true
}

