{
    "serverAuthContext" : {
        "sessionModule" : {
            "name" : "JWT_SESSION",
            "properties" : {
                "sessionOnly" : true,
                "isHttpOnly" : true,
                "enableDynamicRoles" : false,
                "maxTokenLifeSeconds" : "500",
                "tokenIdleTimeSeconds" : "500"
            }
        },
        "authModules" : [
            {
                "name" : "STATIC_USER",
                "properties" : {
                    "queryOnResource" : "internal/user",
                    "username" : "anonymous",
                    "password" : "anonymous",
                    "defaultUserRoles" : [
                        "internal/role/openidm-reg"
                    ]
                },
                "enabled" : true
            },
            {
                "name" : "MANAGED_USER",
                "properties" : {
                    "augmentSecurityContext" : {
                        "type" : "text/javascript",
                        "source" : "require('auth/customAuthz').setProtectedAttributes(security)"
                    },
                    "queryId" : "credential-query",
                    "queryOnResource" : "managed/user",
                    "propertyMapping" : {
                        "authenticationId" : "username",
                        "userCredential" : "password",
                        "userRoles" : "authzRoles"
                    },
                    "defaultUserRoles" : [ ]
                },
                "enabled" : false
            },
            {
                "name" : "INTERNAL_USER",
                "properties" : {
                    "queryId" : "credential-internaluser-query",
                    "queryOnResource" : "internal/user",
                    "propertyMapping" : {
                        "authenticationId" : "username",
                        "userCredential" : "password",
                        "userRoles" : "authzRoles"
                    },
                    "defaultUserRoles" : [ ]
                },
                "enabled" : true
            },
            {
                "name" : "PASSTHROUGH",
                "properties" : {
                    "augmentSecurityContext" : {
                        "type" : "text/javascript",
                        "file" : "auth/populateAsManagedUser.js"
                    },
                    "queryOnResource" : "system/ldap/account",
                    "propertyMapping" : {
                        "authenticationId" : "uid"
                    },
                    "managedUserLink" : "systemLdapAccounts_managedUser",
                    "defaultUserRoles" : [
                        "internal/role/openidm-authorized"
                    ]
                },
                "enabled" : false
            },
            {
                "name" : "SOCIAL_PROVIDERS",
                "properties" : {
                    "defaultUserRoles" : [
                        "internal/role/openidm-authorized"
                    ],
                    "augmentSecurityContext" : {
                        "type" : "text/javascript",
                        "globals" : { },
                        "file" : "auth/populateAsManagedUserFromRelationship.js"
                    },
                    "propertyMapping" : {
                        "userRoles" : "authzRoles"
                    }
                },
                "enabled" : false
            },
            {
                "name" : "STATIC_USER",
                "properties" : {
                    "queryOnResource" : "internal/user",
                    "username" : "&{openidm.prometheus.username}",
                    "password" : "&{openidm.prometheus.password}",
                    "defaultUserRoles" : [
                        "internal/role/openidm-prometheus"
                    ]
                },
                "enabled" : true
            },
            {
                "name" : "OAUTH_CLIENT",
                "properties" : {
                    "augmentSecurityContext" : {
                        "type" : "text/javascript",
                        "globals" : {
                            "sessionValidationBaseEndpoint" : "https://login.dev-walter.amerentest.com/json/ameren/sessions/"
                        },
                        "file" : "auth/amSessionCheck.js"
                    },
                    "propertyMapping" : {
                        "authenticationId" : "uid",
                        "userRoles" : "authzRoles"
                    },
                    "defaultUserRoles" : [
                        "internal/role/openidm-authorized"
                    ],
                    "idpConfig" : {
                        "provider" : "OPENAM",
                        "icon" : "<button class=\"btn btn-lg btn-default btn-block btn-social-provider\"><img src=\"images/forgerock_logo.png\">Sign In</button>",
                        "scope" : [
                            "openid"
                        ],
                        "authenticationIdKey" : "sub",
                        "clientId" : "openidm",
                        "clientSecret" : "openidm",
                        "authorizationEndpoint" : "https://login.dev-walter.amerentest.com/oauth2/realms/root/realms/ameren/authorize",
                        "tokenEndpoint" : "https://login.dev-walter.amerentest.com/oauth2/realms/root/realms/ameren/access_token",
                        "endSessionEndpoint" : "https://login.dev-walter.amerentest.com/oauth2/realms/root/realms/ameren/connect/endSession",
                        "wellKnownEndpoint" : "https://login.dev-walter.amerentest.com/oauth2/ameren/.well-known/openid-configuration",
                        "redirectUri" : "https://openidm.dev-walter.amerentest.com:443",
                        "configClass" : "org.forgerock.oauth.clients.oidc.OpenIDConnectClientConfiguration",
                        "displayIcon" : "forgerock",
                        "enabled" : true
                    },
                    "queryOnResource" : "system/ldap/account"
                },
                "enabled" : false
            }
        ]
    }
}
